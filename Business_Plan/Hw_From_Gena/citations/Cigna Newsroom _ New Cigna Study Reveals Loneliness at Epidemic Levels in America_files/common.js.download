/*Carousel Arrow edit*/
function moveArrows(){
	var len = $(window).width();
	if(len<700){
		var prevVal = 0.25*len;
		var nextVal = 0.75*len-17;
		$(".mod-h4 .flexslider-carousel .flex-direction-nav .flex-prev").css('left', prevVal);
		$(".mod-h4 .flexslider-carousel .flex-direction-nav .flex-next").css('left', nextVal);
	}
	if(len>=700 && len<960){
		var prevVal = 0.35*len;
		var nextVal = 0.65*len-17;
		$(".mod-h4 .flexslider-carousel .flex-direction-nav .flex-prev").css('left', prevVal);
		$(".mod-h4 .flexslider-carousel .flex-direction-nav .flex-next").css('left', nextVal);
	}
	if(len>=960){
		$(".mod-h4 .flexslider-carousel .flex-direction-nav .flex-prev").removeAttr("style");
		$(".mod-h4 .flexslider-carousel .flex-direction-nav .flex-next").removeAttr("style");
	}
}

$(window).on('load', function(){
	var src = "/iwov-resources/images/arrow-new-sprite.png";
	var img = new Image();
	img.src = src;

	img.onload = function() {
		moveArrows();
	}
	if (img.complete){
		img.onload();
	}
});

$(window).resize(function(){
	moveArrows();
});
/*End carousel arrow edit*/

